<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ManageBankcardCommon">

	<select id="getBankcardDetail" resultType="com.wei.orderpay.manage.bean.user.BankcardBean">
		SELECT * FROM bankcard WHERE source = #{source} AND guid = #{guid} and bankCardID = #{bankCardID} 
	</select>
</mapper>


